{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "flatblocks",
  "version-string": "0.0.1",
  "dependencies": [
    "catch2",
    "glfw3",
    "spdlog",
    "glad",
    "imgui",
    {
      "name": "joltphysics",
      "default-features": false,
      "features": [
        "debugrenderer",
        "profiler",
        "rtti"
      ]
    },
    "glm",
    "stb",
    "cxxopts",
    "meshoptimizer",
    {
      "name": "tracy",
      "default-features": false,
      "features": [
        "crash-handler"
      ]
    },
    "openal-soft",
    "sol2",
    "freetype",
    "assimp",
    "nlohmann-json",
    "shaderc",
    "spirv-cross"
  ]
}