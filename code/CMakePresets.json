{
  "version": 9,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 30,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "Default",
      "displayName": "Default Config",
      "description": "Default build using multiconfig VS generator",
      "generator": "Visual Studio 17 2022",
      "binaryDir": "${sourceDir}/../builds/FlatBlocksBin",
      "installDir": "${sourceDir}/../builds/FlatBlocksBuild",
      "toolchainFile": "${sourceDir}/Extern/vcpkg/scripts/buildsystems/vcpkg.cmake",
      "cacheVariables": {
        "CMAKE_CXX_STANDARD": "20",
        "PREPROCESSORS": "$<$<CONFIG:Debug>:EF_RELEASE=0;EF_DEBUG_BUILD=1;EF_LOGS_ENABLED=1>;$<$<CONFIG:RelWithDebInfo>:EF_RELEASE=1;EF_DEBUG_BUILD=0;EF_LOGS_ENABLED=1>;$<$<CONFIG:Release>:EF_RELEASE=1;EF_DEBUG_BUILD=0;EF_LOGS_ENABLED=0>"
      },
      "vendor": {
        "jetbrains.com/clion": {
          "toolchain": "Visual Studio"
        }
      }
    }
  ],
  "buildPresets": [
    {
      "name": "Build",
      "displayName": "Default Build",
      "configurePreset": "Default",
      "vendor": {
        "jetbrains.com/clion": {
          "toolchain": "Visual Studio"
        }
      },
      "jobs": 8,
      "cleanFirst": true,
      "verbose": true
    }
  ]
}
